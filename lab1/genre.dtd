<!ELEMENT movielist (genre, genrefilmer, genredescription, kontakt)>
	<!ELEMENT genre (#PCDATA)>
<!ATTLIST genre type (horror|romance|action) #REQUIRED>
<!ELEMENT genrefilmer (film+)>
	<!ELEMENT film (title, regi, runtime, age, starttime)>
			<!ELEMENT title (#PCDATA)>
			<!ATTLIST title url CDATA #REQUIRED>
            <!ELEMENT regi (#PCDATA)>
			<!ATTLIST regi url CDATA #REQUIRED>
		<!ELEMENT runtime (#PCDATA)>
		<!ELEMENT age (#PCDATA)>
		<!ELEMENT starttime (#PCDATA)>
<!ELEMENT genredescription (#PCDATA)>
<!ELEMENT kontakt (#PCDATA)> 
<!ATTLIST kontakt url CDATA #REQUIRED>
